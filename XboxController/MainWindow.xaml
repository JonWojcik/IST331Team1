			<Button x:Name="btnResume" Grid.Column="1" Grid.ColumnSpan="1"  Grid.Row="1" Grid.RowSpan="1" Content="Resume" BorderBrush="#FF979797" Background="#FF15B215" Foreground="White" FontSize="30" Margin="5px" VerticalContentAlignment="Center" Click="btnResume_Click" />
			<Button x:Name="btnOptions" Grid.Column="1" Grid.ColumnSpan="1"  Grid.Row="2" Grid.RowSpan="1" Content="Options" BorderBrush="#FF979797" Background="#FF008AFD" Foreground="White" FontSize="30" Margin="5px" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Click="btnOptions_Click" />
			<Button x:Name="btnRestart" Grid.Column="1" Grid.ColumnSpan="1"  Grid.Row="3" Grid.RowSpan="1" Content="Restart" BorderBrush="#FF979797" Background="#FFDCDC1E" Foreground="White" FontSize="30" Margin="5px" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
			<Button x:Name="btnQuit" Grid.Column="1" Grid.ColumnSpan="1"  Grid.Row="4" Grid.RowSpan="1" Content="Quit" BorderBrush="#FF979797" Background="#FFFD0000" Foreground="White" FontSize="30" Margin="5px" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
		</Grid>
		<Grid x:Name="calibrationScreen" Background="Black" Opacity="0.85" Visibility="Hidden">
			<TextBlock Text="Please point at the Green dot and press the trigger button" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="50" TextWrapping="Wrap" Foreground="White"/>
			<Ellipse x:Name="topLeft" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="-25 -25" Height="50" Width="50" Fill="Green"/>
			<Ellipse x:Name="bottomLeft" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="-25 -25" Height="50" Width="50" Fill="Gray"/>
			<Ellipse x:Name="bottomRight" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="-25 -25" Height="50" Width="50" Fill="Gray"/>
			<Ellipse x:Name="topRight" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="-25 -25" Height="50" Width="50" Fill="Gray"/>
		</Grid>
        <!-- MAIN STARTUP SCREEN-->
		<Grid x:Name="StartScreen" Background="LightSkyBlue" Visibility="Hidden">
			<Grid.RowDefinitions>
				<RowDefinition Height="*"/>
				<RowDefinition Height="*"/>
				<RowDefinition Height="*"/>
				<RowDefinition Height="*"/>
				<RowDefinition Height="*"/>
				<RowDefinition Height="*"/>
			</Grid.RowDefinitions>
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="100px"/>
				<ColumnDefinition Width="*"/>
				<ColumnDefinition Width="100px"/>
			</Grid.ColumnDefinitions>
			<TextBlock Text="Game Title" Foreground="White" FontSize="35" Grid.Row="0" Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Center"/>
			<Button x:Name="StartBtn" Grid.Column="1" Grid.ColumnSpan="1"  Grid.Row="1" Grid.RowSpan="1" Content="Start" BorderBrush="#FF979797" Background="#FF04DC18" Foreground="White" FontSize="30" Margin="5px" VerticalContentAlignment="Center" />
			<Button Grid.Column="1" Grid.ColumnSpan="1"  Grid.Row="2" Grid.RowSpan="1" Content="Settings" BorderBrush="#FF979797" Background="#FFCFA900" Foreground="White" FontSize="30" Margin="5px" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
			<Button Grid.Column="1" Grid.ColumnSpan="1"  Grid.Row="3" Grid.RowSpan="1" Content="High Scores" BorderBrush="#FF979797" Background="#FF008AFD" Foreground="White" FontSize="30" Margin="5px" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
			<Button Grid.Column="1" Grid.ColumnSpan="1"  Grid.Row="4" Grid.RowSpan="1" Content="Quit" BorderBrush="#FF979797" Background="#FFA80404" Foreground="White" FontSize="30" Margin="5px" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
		</Grid>
        <!--Section for debugging tools-->
		<Canvas x:Name="debugging" Visibility="Hidden">
			<TextBlock x:Name="HeadPosition" Grid.Row="0"  HorizontalAlignment="Left" Padding="25px 0px" TextWrapping="Wrap" Text="HeadPosition:" VerticalAlignment="Center" Canvas.Left="10" Canvas.Top="76"/>
			<TextBlock x:Name="RightHandPosition" Grid.Row="0"  HorizontalAlignment="Right" Padding="25px 0px" TextWrapping="Wrap" Text="RightHandPosition:" VerticalAlignment="Center" Canvas.Top="55"/>
			<TextBlock x:Name="LeftHandPosition" Grid.Row="0"  HorizontalAlignment="Right" Padding="25px 0px" TextWrapping="Wrap" Text="LeftHandPosition:" VerticalAlignment="Center" Canvas.Top="92"/>
            <TextBlock x:Name="handDirection" Grid.Row="0"  HorizontalAlignment="Right" Padding="25px 0px" TextWrapping="Wrap" Text="Direction" VerticalAlignment="Center" Canvas.Top="113"/>
			<Ellipse x:Name="ellipiseHandRight" Fill="Red" HorizontalAlignment="Left" Height="50" Margin="387,269,0,-47" Stroke="Black" VerticalAlignment="Top" Width="50" Opacity="0.75"/>
			<Ellipse x:Name="ellipiseHandLeft" Fill="Blue" HorizontalAlignment="Left" Height="50"  Stroke="Black" VerticalAlignment="Top" Width="50" Opacity="0.75"/>

		</Canvas>
        <!-- OPTIONS MENU -->
		<Grid x:Name="OptionsMenu" Opacity="0.75" Background="Black" Visibility="Hidden">
			<Grid.RowDefinitions>
				<RowDefinition Height="*"/>
				<RowDefinition Height="*"/>
				<RowDefinition Height="*"/>
				<RowDefinition Height="*"/>
				<RowDefinition Height="*"/>
				<RowDefinition Height="*"/>
			</Grid.RowDefinitions>
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="100px"/>
				<ColumnDefinition Width="*"/>
				<ColumnDefinition Width="100px"/>
			</Grid.ColumnDefinitions>
			<TextBlock Text="Options Menu" Foreground="White" FontSize="35" Grid.Row="0" Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Center"/>
			<Button x:Name="btnStartMenu" Grid.Column="1" Grid.ColumnSpan="1"  Grid.Row="2" Grid.RowSpan="1" Content="Start Menu" BorderBrush="#FF979797" Background="#FF1EDC27" Foreground="White" FontSize="30" Margin="5px" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
			<Button x:Name="btnBack" Grid.Column="1" Grid.ColumnSpan="1"  Grid.Row="4" Grid.RowSpan="1" Content="Back" BorderBrush="#FF979797" Background="#FFFD0000" Foreground="White" FontSize="30" Margin="5px" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Click="btnBack_Click" />
		</Grid>

		

	</Grid>
</Window>